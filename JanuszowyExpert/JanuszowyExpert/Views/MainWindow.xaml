<Window      
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JanuszowyExpert"
        xmlns:ViewModels="clr-namespace:JanuszowyExpert.ViewModels" 
    x:Class="JanuszowyExpert.MainWindow"
    
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">

    <Window.DataContext>
        <ViewModels:MainViewModel/>
    </Window.DataContext>
    <Grid Background="Green">
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="66,62,0,0" TextWrapping="Wrap" Text="{Binding Questions.QuestionContext}" VerticalAlignment="Top" Height="60" Width="304" FontSize="40"/>
    </Grid>
</Window>
